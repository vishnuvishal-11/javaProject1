<!--						<configuration>-->
<!--							<failOnError>true</failOnError>-->
<!--							<enableLazyInitialization>true</enableLazyInitialization>-->
<!--						</configuration>-->
<!--						<goals>-->
<!--							<goal>enhance</goal>-->
<!--						</goals>-->
<!--					</execution>-->
<!--				</executions>-->
<!--			</plugin>-->
		</plugins>
	</build>

</project>
