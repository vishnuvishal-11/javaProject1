	<groupId>com.example</groupId>
	<artifactId>DbprojectApplication</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>DbprojectApplication</name>
	<description>Demo project for Spring Boot with db</description>
	<properties>
		<java.version>18</java.version>
		<jaxb.version>2.4.0-b180830.0359</jaxb.version>
<!--		<liquibase.propertyFile>${project.basedir}/application.properties</liquibase.propertyFile>-->
	</properties>
	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-actuator</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-rest</artifactId>
		</dependency>
		<dependency>
